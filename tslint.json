{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "no-empty": false,
    "variable-name": false,
    "class-name": true,
    "curly": false,
    "import-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "case-insensitive"
      }
    ]
  },
  "rules": {
    "prefer-array-literal": false,
    "no-empty": false,
    "no-namespace": false,
    "no-empty-interface": false,
    "interface-name": false,
    "variable-name": false,
    "class-name": true,
    "curly": false,
    "import-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "case-insensitive"
      }
    ]
  },
  "rulesDirectory": []
}
