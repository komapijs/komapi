{
  "name": "komapi",
  "version": "0.0.8",
  "description": "Komapi.js is an opinionated Node.js framework built on top of Koa 2.0",
  "author": "ersims",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/komapijs/komapi.git"
  },
  "keywords": [
    "komapi",
    "koa",
    "opinionated",
    "framework",
    "rest",
    "restful",
    "api",
    "app"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/komapijs/komapi/issues"
  },
  "homepage": "https://github.com/komapijs/komapi#README.md",
  "engines": {
    "node": ">= 4.4.5",
    "npm": ">= 2.15.5"
  },
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean --silent",
    "build": "babel -q -D -s true ./src/ --out-dir ./dist/",
    "check-deps": "nsp check --output summary && ncu --packageFile ./package.json",
    "clean": "del-cli ./dist ./coverage ./.nyc_output",
    "coverage": "nyc report --reporter=text --reporter=text-summary --reporter=html && nyc check-coverage --lines=100 --functions=100 --branches=100 --statements=100",
    "lint": "eslint index.js src/",
    "pretest": "npm run clean --silent && npm run lint --silent",
    "test": "nyc --reporter=lcov ava --require babel-core/register --timeout 180s --concurrency=25",
    "posttest": "npm run coverage --silent",
    "prepublish": "npm run build",
    "update-deps": "ncu --packageFile ./package.json -u --upgradeAll",
    "watch": "ava --require babel-core/register --watch --serial --verbose"
  },
  "dependencies": {
    "ajv": "^4.4.0",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.1",
    "boom": "^3.2.2",
    "bunyan": "^1.8.1",
    "delegates": "^1.0.0",
    "file-exists": "^2.0.0",
    "get-parameter-names": "^0.3.0",
    "helmet": "^2.1.2",
    "install": "^0.8.1",
    "joi": "^9.0.4",
    "kcors": "^2.1.1",
    "knex": "0.11.7",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-compose": "^3.1.0",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-mount": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.0.1",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "node-uuid": "^1.4.7",
    "objection": "^0.5.5",
    "passport": "^0.3.2",
    "recursive-readdir-sync": "^1.0.6",
    "source-map-support": "^0.4.2"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-security": "^1.2.0",
    "handlebars": "^4.0.5",
    "npm-check-updates": "^2.8.0",
    "nsp": "^2.6.1",
    "nyc": "^7.1.0",
    "passport-anonymous": "^1.0.1",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "sqlite3": "^3.1.4",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0"
  },
  "files": [
    "dist",
    "src"
  ]
}
